

<section>

  <li data-book-id="<%= book.ISBN %>">
    <div class="book-container">
      <img src=<%= book.image_url %>>
      <section>
        <h3><%= book.title %></h3>
        <span><%= book.author %></span>
        <p class="desc"><%= book.description %></p>        
      </section>
  </li>
  <button ><a href="/books/<%= book.id %>">Select This Book</a></button>
  <div>
    <button ><a href="/">Book List</a></button>
  </div>
</section>


   